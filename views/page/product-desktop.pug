extends ../wrapper/page

append header
    //- #CATEGORY
    section.c-section(data-section='Category List')
        .c-cat-list.c-cat-list--horizontal.c-cat-list--dark(class='[ ]')
            ul.o-carousel.o-carousel--8col.o-carousel--small.u-margin-bottom-none(class='[ ]')
                each category in categories
                    li.o-carousel__item.c-cat-list__item.u-txt-align-center(id=category._id)
                        a.c-cat-list__item-detail(href='/category/#{category.name}')
                            i(class=category.iconClass + ' [ large ]', title=category.name)
                            |#{category.name}
    //- /Category





//- #BLOCK: BODY
