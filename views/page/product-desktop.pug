                                    img(data-lazy=product.imUrl)
                                .c-slider__nav-item
                                    img(data-lazy=product.imUrl)
                                .c-slider__nav-item
                                    img(data-lazy=product.imUrl)


                    //-#DETAIL PRODUCT
                    section.o-layout__item(class='u-4/10 u-push-1/20', data-section='Product Detail')

                        //-#NAME AND CATEGORY
                        .u-txt--title.u-txt--dark.u-txt--bold.u-margin-bottom-small=product.title

                        //-#SMALL PRODUCT DETAIL
                        .u-margin-top-small.u-margin-bottom-tiny.u-cf
                            //-#BRAND NAME
                            span.u-txt--tiny(class='[ ]')
                                | Brand:
                                |

                            //- If the product has a brand
                            if (product.brand)
                                span.u-txt--bright.u-txt--xbold(class='[ ]')=product.brand


                            //-#RATING AND RATING COUNTER
                            .u-float-right.u-d-flex.u-fd--column(class='[ ]')
                                .star
                                    i.fas.fa-star.tiny.rect(class='[ ]', data-rating='1')
                                    i.fas.fa-star.tiny.rect(class='[ ]', data-rating='2')
                                    i.fas.fa-star.tiny.rect(class='[ ]', data-rating='3')
                                    i.fas.fa-star.tiny.rect(class='[ ]', data-rating='4')
                                    i.fas.fa-star.tiny.rect(class='[ ]', data-rating='5')
                                span.u-txt--tiny.u-txt--light(class='[ ]')=product.rating


